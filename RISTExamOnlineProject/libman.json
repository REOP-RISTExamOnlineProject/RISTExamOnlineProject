{
  "version": "1.0",
  "defaultProvider": "cdnjs",
  "libraries": [
    {
      "library": "twitter-bootstrap@4.1.3",
      "destination": "wwwroot/lib/bootstrap/"
    },
    {
      "library": "font-awesome@5.13.0",
      "destination": "wwwroot/lib/font-awesome/"
    },
    {
      "library": "html5shiv@3.7.3",
      "destination": "wwwroot/lib/html5shiv/"
    },
    {
      "library": "respond.js@1.4.2",
      "destination": "wwwroot/lib/respond.js/"
    },
    {
      "library": "popper.js@2.4.0",
      "destination": "wwwroot/lib/popper.js/"
    },
    {
      "provider": "cdnjs",
      "library": "jquery@3.5.1",
      "destination": "wwwroot/lib/jquery/"
    },

    {
      "provider": "unpkg",
      "library": "@aspnet/signalr@3.0.0-preview6.19307.2",
      "destination": "wwwroot/lib/signalr/",
      "files": [
        "dist/browser/signalr.js"
      ]
    }
  ]
}